#search
  .search-form-wrap
    = form_tag search_path, :id => "search-form", :method => :get do
      = hidden_field_tag 's', 1
      = text_field_tag "q", t("searches.form.search_anything"), :id => "search-field", :class => "inactive", "data-autocomplete-url" => autocomplete_url
      %input{:type => "submit", :value => t('searches.index.form.send')}
      %ul#search-results.autocomplete-list




